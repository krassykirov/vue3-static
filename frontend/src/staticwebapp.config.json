{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/products",
      "rewrite": "/favorites"
    },
    {
      "route": "/category*",
      "rewrite": "/"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }

  ]
}