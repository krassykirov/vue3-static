{
  "navigationFallback": {
  "rewrite": "index.html"
  },
  "routes": [
    {
      "route": "/products",
      "rewrite": "/favorites"
    },
    {
      "route": "/category*",
      "rewrite": "/cart"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".json": "text/json"
  }
}